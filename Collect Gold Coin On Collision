using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class CollectCoins : MonoBehaviour
{
    //When player makes contact with coin = destroyed ("PickedUp").
    void OnTriggerEnter()
    {
        GameObject.Destroy(gameObject);
        GameObject.FindGameObjectWithTag("Finish").GetComponent<SCR_OpenMaze>().AddCoins();
    }
}
